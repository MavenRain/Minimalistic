<?xml version="1.0" encoding="utf-8"?>
<ServiceDefinition name="VSOAccessTokenWorkerRole" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition" schemaVersion="2014-06.2.4">
  <WorkerRole name="AccessTokenWorkerRole" vmsize="Small">
    <ConfigurationSettings>
      <Setting name="TokenRequestUrl" />
      <Setting name="TokenStorageUrl" />
      <Setting name="AppId" />
      <Setting name="AppSecret" />
      <Setting name="CallbackUrl" />
      <Setting name="StorageConnectionString" />
    </ConfigurationSettings>
    <Endpoints>
      <InputEndpoint name="Default" protocol="http" port="5453" localPort="5453" />
    </Endpoints>
  </WorkerRole>
</ServiceDefinition>